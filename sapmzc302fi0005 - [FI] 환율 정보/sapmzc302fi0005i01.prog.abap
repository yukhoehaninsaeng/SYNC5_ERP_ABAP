MODULE exit INPUT.

  CALL METHOD : go_grid_exr->free, go_grid_log->free,
                go_cont_exr->free, go_cont_log->free.
  FREE : go_grid_exr, go_grid_log, go_cont_exr, go_cont_log,
         go_cont_usa, go_cont_chn, go_cont_jpn, go_cont_eur.

  LEAVE TO SCREEN 0.

ENDMODULE.
