*       text
*----------------------------------------------------------------------*
MODULE user_command_0100 INPUT.
  CASE gv_okcode.
    WHEN 'TMPL'.
      PERFORM template_download.
    WHEN 'XLSU'.
      CALL SCREEN 110 STARTING AT 05 03.
    WHEN 'PDFD'.
      PERFORM select_folder.
      PERFORM pdf_download.
  ENDCASE.
ENDMODULE.
